{
    "name": "yekta/laravel-auth-package-discovery-fix",
    "description": "Fix for Laravel AuthManager 'Auth driver not defined' error during package discovery",
    "keywords": ["laravel", "auth", "package-discovery", "fix", "composer"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yekta\\LaravelAuthPackageDiscoveryFix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yekta\\LaravelAuthPackageDiscoveryFix\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs-fix": "php-cs-fixer fix",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "pint": "pint",
        "pint-test": "pint --test",
        "phpstan": "phpstan analyse",
        "quality": [
            "@cs-check",
            "@phpstan",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yekta\\LaravelAuthPackageDiscoveryFix\\AuthPackageDiscoveryFixServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
