{
    "name": "yekta/laravel-auth-package-discovery-fix",
    "description": "Fix for Laravel AuthManager 'Auth driver not defined' error during package discovery",
    "keywords": ["laravel", "auth", "package-discovery", "fix", "composer"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yekta\\LaravelAuthPackageDiscoveryFix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yekta\\LaravelAuthPackageDiscoveryFix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yekta\\LaravelAuthPackageDiscoveryFix\\AuthPackageDiscoveryFixServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
